"vertexlitgeneric"
{
 	"$model" 1
	"$basetexture" "models/bottle_mel/liquid"
 	//"$refractamount" ".001"
 	//"$bluramount" "3"
	//"$translucent" 1
	//"$REFRACTTINT" "{0 180 255}"
	"$scale" "[1 1]"
	"$dudvmap" "dev/water_dudv"
 	"$normalmap" "dev/water_normal"
	"$bumpmap"  "dev/waterA_normal"
 	"$surfaceprop" "water"
	"$dudvframe" "0"
	"$bumpframe" "0"
//	"$nocull" 1
	//"$REFRACTTINTTEXTURE" "models/bottle_mel/liquid"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$dudvmap"
			"animatedtextureframenumvar" "$dudvframe"
			"animatedtextureframerate" 30.00
		}

		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" .010
			"texturescrollangle" -35.00
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}

		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" .05
			"texturescrollangle" 45.00
		}

	}
}
